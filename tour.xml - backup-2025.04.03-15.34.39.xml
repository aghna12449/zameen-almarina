	<scene name="scene_AL_MARINA_aerial" title="AL MARINA aerial" onstart="" thumburl="panos/AL_MARINA_aerial.tiles/thumb.jpg" lat="" lng="" heading="">
		
		<control bouncinglimits="calc:image.cube ? true : false" />

		<view hlookat="0.0" vlookat="0.0" fovtype="MFOV" fov="120" maxpixelzoom="2.0" fovmin="70" fovmax="140" limitview="auto" />

		<preview url="panos/AL_MARINA_aerial.tiles/preview.jpg" />

		<image hfov="1.00" vfov="0.562500" voffset="0.00">
			<flat url="panos/AL_MARINA_aerial.tiles/l%l/%v/l%l_%v_%h.jpg" multires="512,1024x578,2048x1152,4096x2304" />
		</image>

	</scene>

